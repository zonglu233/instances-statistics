	# 获取时间区间
	period = (start, end)->
		now_date = new Date()
		if not start
			year = now_date.getFullYear()
			month = now_date.getMonth()+1
			start = new Date("'" + year + "-" + month + "'")

		if not end
			end = now_date

		obj = {}
		obj.start = start
		obj.end = end
		return obj

	period_obj = period("", "")

	console.log period_obj